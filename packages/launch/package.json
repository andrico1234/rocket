{
  "name": "@rocket/launch",
  "version": "0.20.0-alpha.0",
  "publishConfig": {
    "access": "public"
  },
  "description": "Launch Preset for Rocket",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/modernweb-dev/rocket.git",
    "directory": "packages/launch"
  },
  "author": "Modern Web <hello@modern-web.dev> (https://modern-web.dev/)",
  "homepage": "https://rocket.modern-web.dev/docs/presets/launch/",
  "main": "./src/index.js",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist-types/src/index.d.ts",
      "default": "./src/index.js"
    },
    "./assets/*": "./assets/*",
    "./css/*": "./css/*",
    "./js/*": "./src/public/*",
    "./inline-notification": {
      "types": "./dist-types/src/inline-notification/index.d.ts",
      "default": "./src/inline-notification/index.js"
    },
    "./inline-notification/define": "./src/inline-notification/define.js",
    "./inline-notification/inline-notification.js": "./src/inline-notification/define.js"
  },
  "scripts": {
    "analyze": "cem analyze --litelement",
    "test": "mocha --timeout 5000 test-node/**/*.test.{js,cjs} test-node/*.test.{js,cjs}",
    "test:watch": "onchange 'src/**/*.{js,cjs}' 'test-node/**/*.{js,cjs}' -- npm test"
  },
  "files": [
    "*.js",
    "assets",
    "css",
    "inline-notification",
    "js",
    "preset",
    "src"
  ],
  "keywords": [
    "rocket",
    "preset"
  ],
  "dependencies": {
    "@rocket/drawer": "^0.1.3",
    "@rocket/engine": "^0.1.0-alpha.8",
    "lit": "^2.0.0",
    "workbox-window": "^6.1.5"
  },
  "types": "./dist-types/src/index.d.ts",
  "customElements": "custom-elements.json",
  "typesVersions": {
    "*": {
      "*": [
        "./dist-types/src/index.d.ts"
      ],
      "inline-notification": [
        "./dist-types/src/inline-notification/index.d.ts"
      ]
    }
  }
}
